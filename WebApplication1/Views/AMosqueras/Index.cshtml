@model IEnumerable<WebApplication1.Models.AMosquera>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.numeroFavorito)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.apodo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.genero)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.fecha)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.numeroFavorito)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.apodo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.genero)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.fecha)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.edad">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.edad">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.edad">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
